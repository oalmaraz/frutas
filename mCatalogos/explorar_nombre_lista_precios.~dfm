  inherited qyExplorar: TIBQuery
    SQL.Strings = (
      
        'SELECT N.ID R_ID, N.DESCRIPCION R_DESCRIPCION, N.VIGENCIA_DIAS R' +
        '_VIGENCIA_DIAS,'
      '               M.CLAVE R_M_CLAVE, M.DESCRIPCION R_M_DESCRIPCION'
      
        'FROM NOMBRE_LISTA_PRECIOS N LEFT JOIN MONEDAS M ON N.MONEDA = M.' +
        'ID'
      'WHERE N.ID = 0')
    object qyExplorarR_ID: TIntegerField
      FieldKind = fkInternalCalc
      FieldName = 'R_ID'
      ReadOnly = True
    end
    object qyExplorarR_DESCRIPCION: TIBStringField
      FieldKind = fkInternalCalc
      FieldName = 'R_DESCRIPCION'
      ReadOnly = True
      Size = 100
    end
    object qyExplorarR_VIGENCIA_DIAS: TIntegerField
      FieldKind = fkInternalCalc
      FieldName = 'R_VIGENCIA_DIAS'
      ReadOnly = True
    end
    object qyExplorarR_M_CLAVE: TIBStringField
      FieldName = 'R_M_CLAVE'
      Origin = 'MONEDAS.CLAVE'
      Size = 10
    end
    object qyExplorarR_M_DESCRIPCION: TIBStringField
      FieldName = 'R_M_DESCRIPCION'
      Origin = 'MONEDAS.DESCRIPCION'
      Size = 50
    end
